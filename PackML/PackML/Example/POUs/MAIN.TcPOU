  <POU Name="MAIN" Id="{80fd576e-8bda-0001-19b1-22d89fc11873}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM MAIN
VAR
	fbModule:		FB_Module01;	
	bReset:			BOOL;
	bStart: 		BOOL;
    bHold:       	BOOL;
    bPause:    		BOOL;
	bStop:       	BOOL;
    bAbort:      	BOOL;

END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
fbSaftey();
fbModule();

IF bReset THEN
	bReset:= NOT fbModule.M_Request(E_Request.Reset);
END_IF

IF bStart THEN
	bStart:= NOT fbModule.M_Request(E_Request.Start);
END_IF

IF bStop THEN
	bStop:= fbModule.M_Request(E_Request.Stop);
END_IF

IF bHold THEN
	bHold:= NOT fbModule.M_Request(E_Request.Hold);
END_IF

IF bPause THEN
	bPause:= NOT fbModule.M_Request(E_Request.Pause);
END_IF

IF bAbort THEN
	bAbort:= NOT fbModule.M_Request(E_Request.Abort);
END_IF

]]></ST>
